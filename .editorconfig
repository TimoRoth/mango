root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space

[*.md]
indent_size = 4
trim_trailing_whitespace = false
insert_final_newline = true

[*.{vcxproj,props,targets}]
charset = utf-8-bom

[*.{c,h,inc}]
trim_trailing_whitespace = true
insert_final_newline = true

[Makefile]
indent_style = tab
